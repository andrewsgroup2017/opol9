{
	"name": "opol9",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"dev": "npm run electron-serve",
		"prod": "npm run electron-prod"
	},
	"dependencies": {
		"@capacitor/android": "^1.0.0-beta.3",
		"@capacitor/cli": "^1.0.0-beta.3",
		"@capacitor/core": "^1.0.0-beta.3",
		"font-awesome": "^4.7.0",
		"lodash.truncate": "^4.4.2",
		"lowdash": "^1.2.0",
		"nprogress": "^0.2.0",
		"pouchdb-authentication": "^1.1.3",
		"pouchdb-browser": "^7.0.0",
		"pouchdb-find": "^7.0.0",
		"pouchdb-live-find": "^0.4.0",
		"register-service-worker": "^1.0.0",
		"split.js": "^1.3.5",
		"stylus-loader": "^3.0.2",
		"vee-validate": "^2.1.0-beta.7",
		"vue": "^2.5.16",
		"vue-perfect-scrollbar": "^0.1.0",
		"vue-pouch": "0.0.23",
		"vue-router": "^3.0.1",
		"vuetify": "^1.0.19",
		"vuex": "^3.0.1"
	},
	"devDependencies": {
		"@babel/polyfill": "^7.0.0-beta.49",
		"@vue/cli-plugin-babel": "^3.0.0-rc.10",
		"@vue/cli-plugin-eslint": "^3.0.0-rc.10",
		"@vue/cli-plugin-pwa": "^3.0.0-rc.10",
		"@vue/cli-service": "^3.0.0-rc.10",
		"@vue/eslint-config-prettier": "^3.0.0-rc.10",
		"babel-plugin-transform-imports": "^1.4.1",
		"electron": "^2.0.2",
		"electron-builder": "^20.18.0",
		"electron-devtools-installer": "^2.2.4",
		"lint-staged": "^7.2.0",
		"material-design-icons-iconfont": "^3.0.3",
		"stylus": "^0.54.5",
		"vue-cli-plugin-electron-builder": "^1.0.0-beta.6",
		"vue-template-compiler": "^2.5.16"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [ "plugin:vue/essential", "@vue/prettier" ],
		"rules": {},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [ "> 1%", "last 2 versions", "not ie <= 8" ],
	"gitHooks": {
		"pre-commit": "lint-staged"
	},
	"lint-staged": {
		"*.js": [ "vue-cli-service lint", "git add" ],
		"*.vue": [ "vue-cli-service lint", "git add" ]
	},
	"main": "dist_electron/bundled/background.js",
	"author": {
		"name": "Canh Kieu",
		"email": "canhkieu@azwebplus.com"
	},
	"build": {
		"appId": "com.example.app",
		"productName": "Electron App",
		"copyright": "Copyright Â© 2018",
		"dmg": {
			"contents": [
				{
					"x": 410,
					"y": 150,
					"type": "link",
					"path": "/Applications"
				},
				{
					"x": 130,
					"y": 150,
					"type": "file"
				}
			]
		},
		"directories": {
			"output": "build"
		},
		"files": [ "dist/**/*", "electron/*", "build/icons/icon.*" ],
		"mac": {
			"icon": "build/icons/icon.icns"
		},
		"win": {
			"icon": "build/icons/icon.ico"
		},
		"linux": {
			"icon": "build/icons"
		}
	}
}
